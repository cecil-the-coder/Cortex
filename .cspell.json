  "ignorePaths": [
    "node_modules/**",
    ".git/**",
    "dist/**",
    "build/**"
  ],
  "ignoreRegExpList": [
    // Base64 encoded strings
    "/[A-Za-z0-9+/]{20,}={0,2}/g",
    // UUIDs and similar IDs
    "/\\b[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\\b/g",
    // Hex strings
    "/\\b[0-9a-fA-F]{16,}\\b/g",
    // API keys and tokens (patterns)
    "/\\b[a-zA-Z0-9_-]{20,}\\b/g"
  ]
